{
    "version": 2,
  "builds": [
    {
      "src": "pubspec.yaml",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build/web"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "flutter build web --release --web-renderer canvaskit --dart-define=FLUTTER_WEB_USE_SKIA=false",
  "outputDirectory": "build/web",
  "framework": null
}
